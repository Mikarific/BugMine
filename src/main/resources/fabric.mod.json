{
  "schemaVersion": 1,
  "id": "bugmine",
  "version": "${version}",
  "name": "BugMine",
  "description": "A collection of various bug fixes and QoL features for 25w14craftmine.",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/bugmine/icon.png",
  "environment": "*",
  "entrypoints": {
